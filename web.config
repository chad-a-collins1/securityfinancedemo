<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<!-- needed to add this to allow PATCH becuase IIS blocks it by default -->
		<handlers>
			<remove name="aspNetCore" />
			<add name="aspNetCore" path="*" verb="GET,HEAD,POST,PUT,DELETE,PATCH,OPTIONS"
				 modules="AspNetCoreModuleV2" resourceType="Unspecified"
				 requireAccess="Script" />
		</handlers>

		<modules>
			<remove name="WebDAVModule" />
		</modules>
		<handlers>
			<remove name="WebDAV" />
		</handlers>

		<aspNetCore processPath="dotnet" arguments=".\HighThroughputApi.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" />
	</system.webServer>
</configuration>
